-#else
 typedef unsigned int ptr_int;
 #define PTR_IS_32 1
-#endif
 
 /* don't redefine typedef's on Solaris 2.6 or Later */
 
-#if !defined(_ILP32) && !defined(_LP64)
-
-#ifndef	_UINT64_T
-#define	_UINT64_T
-#ifdef LONG_IS_64
-typedef unsigned long uint64_t;
-#else
 typedef unsigned long long uint64_t;
-#endif
