check_kernel_headers()
{
	cat > conftest.c << EOF

#include <linux/version.h>

int main(int argc, char *argv[])
{
	printf(UTS_RELEASE"\n");
	exit(0);
}
EOF
	cc conftest.c -o conftest
	if [ test `uname -r` != `./conftest` ];then 
		echo 1>&2
		echo "       WARNING:  Kernel include files do not match the current kernel" 1>&2
		echo 1>&2
	fi
	rm -f conftest.c
	rm -f conftest
}
fix_comm_jar()
{
	if test -f $1;then
		echo "------------------------------------------------------" 1>&2
		echo "WARNING:  A comm.jar file is alread installed" 1>&2
		echo "make install will overwrite it" 1>&2
		echo 1>&2
		echo $1
		echo 1>&2
		echo "------------------------------------------------------" 1>&2
	fi;
}

